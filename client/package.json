{
    "name": "online-fitness-trainer-fe",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview --host",
        "lint": "npm run lint:eslint && npm run lint:filenames",
        "lint:eslint": "eslint . --ext .ts,.tsx,.js,.jsx --report-unused-disable-directives --max-warnings 0",
        "lint:filenames": "node ./validate-filenames.mjs",
        "lint:fix": "npm run lint -- --fix",
        "format": "prettier --write . --config ../.prettierrc",
        "format:check": "prettier --check . --config ../.prettierrc",
        "type-check": "tsc --noEmit --skipLibCheck",
        "pre-commit": "npm run format && npm run type-check && npm run lint"
    },
    "dependencies": {
        "@ant-design/v5-patch-for-react-19": "^1.0.3",
        "antd": "^5.28.1",
        "axios": "^1.13.2",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-router-dom": "^7.9.6",
        "zustand": "^5.0.8"
    },
    "devDependencies": {
        "@eslint/css": "^0.14.1",
        "@eslint/js": "^9.39.1",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@vitejs/plugin-react-swc": "^4.2.1",
        "eslint": "^9.39.1",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "eslint-plugin-unicorn": "^62.0.0",
        "fast-glob": "^3.3.3",
        "globals": "^16.5.0",
        "prettier": "^3.6.2",
        "react-error-boundary": "^6.0.0",
        "react-hook-form": "^7.66.1",
        "sass": "^1.94.0",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.46.3",
        "vite": "^7.2.2",
        "vite-tsconfig-paths": "^5.1.4"
    }
}
